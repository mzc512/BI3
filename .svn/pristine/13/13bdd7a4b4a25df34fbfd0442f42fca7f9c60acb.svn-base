html, body, div, strong, pre, table, tr, td {
    margin: 0;
    padding: 0;
}
html, body{
    overflow-x: hidden;
}
a{
    color: #000;
}
.member_all{
    position: relative;
    background:#eef1f8;
}
.member_bjbs{
    background: #ffffff !important;
}
.member_wth_12{
    clear:both;
}
.hei350{
    height: 350px;
}
.current-tabs-wrap{
    display: block;
    margin-top: 46px;
}
.current-tabs-wrap .enlarge-btn{
    background: url("/anaweb/images/echart/enlarge.png");
}

.member_wth_12{
    width: 100%;
    padding: 12px 0px 0 12px;
    clear: both;
}
.chart-box-allfixed{
    padding-right: 0;
    padding-left: 0;
}
.chart-box-rightfixed{
    padding-right: 0;
    padding-left:12px;
}
.member_padding0{
    padding: 0;
    position: relative;
}

.member_padding0 .enlarge-btn{
    width: 16px;
    height: 16px;
    position: absolute;
    right: 4px;
    top: 35px;
    border: solid 1px #eee;
    z-index: 1;
    display: none;
}

.searchbox {
    position: fixed;
    height: 46px;
    background: #fff;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 2;
}
.searchbox-warp{
    position: absolute;
    top: 6px;
    right: 14px;
}
.searchbox-warp span{
    font-size: 13px;
}

.searchbox-warp input {
    border: none;
    border-bottom: 1px solid #dddddd;
    vertical-align: top;
    height: 30px;
    font-size: 13px;
    width: 200px;
    background-color: transparent;
}

.searchbox-warp .f-blue-btn {
    display: inline-block;
    background: #7FDBFF;
    height: 30px;
    line-height: 30px;
    text-align: center;
    font-size: 13px;
    border: none;
    color: #fff;
    border-radius: 3px;
    cursor: pointer;
    padding: 0 10px;
    vertical-align: top;
}


.searchbox-warp .redsinput{
    color: red;
}
.searchbox-warp .bluesinput{
    color: blue;
}

.resume_div_wrap {
    text-align: left;
    font-family: Arial, "\5b8b\4f53";
    font-size: 12px;
    line-height: 22px;
    color: #333;
    margin: 0 auto;
    overflow: hidden;
    word-break: normal;
    word-wrap: break-word;
    padding-bottom: 8px;
}

.resume_div_wrap table, .resume_div_wrap td {
    font-family: Arial, "\5b8b\4f53";
    font-size: 12px;
    line-height: 22px;
    color: #333;
}

.resume_div_wrap td.common_left {
    width: 8%;
    text-align: right;
    color: #666;
    white-space: nowrap;
}
.common_right_r {
    width: 43%;
}

.common_con {
    padding: 15px 25px 0;
}


.resume_div_wrap td.common_tit ,.common_tit {
    background: #fafafa;
    padding: 0 8px;
    line-height: 30px;
    border: solid 1px #e4e4e4;
    font-size: 18px;
    font-weight: bolder;
    font-family: sans-serif;
}

.common_tit {
    cursor: pointer;
    height: 24px;
}

.hyperlinks, .hyperlinks:link, .hyperlinks:visited, .hyperlinks:active {
    color: #333;
    text-decoration: none;
}

.black-screen-shade{
    background: #000000;
    opacity: 0.8;
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    z-index: 2;
}

.pop-chart-wrap{
    position: fixed;
    background-color: #fff;
    z-index: 2;
    border: solid 1px #5e5e5e;
    top: 10%;
    left: 50%;
    width: 90%;
    margin-left: -45%;
    border-radius: 6px;
}
.pop-chart-wrap .close-btn{
    position: absolute;
    top: 12px;
    right: 4px;
    text-align: center;
    font-size: 4rem;
    width: 30px;
    height: 30px;
    line-height: 30px;
    font-weight: normal;
    cursor: pointer;
    color: #5e5e5e;
}

.pop-chart-wrap .modal-header{
    background: #eee;
    font-weight: bold;
    font-size: 16px;
}
.pop-chart-wrap .modal-header .title{
    height: 28px;
    display: block;
    line-height: 28px;
}

.pop-chart-wrap .pop-chart{
    width: 100%;
}