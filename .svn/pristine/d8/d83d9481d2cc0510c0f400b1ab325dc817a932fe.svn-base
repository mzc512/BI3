@charset "utf-8";
/* CSS Document */
html {
    overflow: hidden
}

body {
    padding: 0;
    margin: 0;
    border: 0;
    font-family: "Hiragino Sans GB", "Microsoft Yahei", Arial, sans-serif;
    font-size: 16px
}

ul, li {
    list-style: none;
    padding: 0;
    margin: 0
}

a {
    text-decoration: none !important;
}

a:link {
    text-decoration: none;
}

a:visited {
    text-decoration: none;
}

a:hover {
    text-decoration: none;
}

a:active {
    text-decoration: none;
}

.nav-top {
    width: 100%;
    height: 50px;
    background: #2e2e2e;
    z-index: 1;
    position: absolute;
    top: 0;
}

.nav-top .system-name {
    margin-left: 8px;
    background-image: url(/anaweb/images/bi_logo3.png);
    width: 179px;
    margin-top: 2px;
    height: 45px;
}

.nav-top .top-tab span {
    float: left;
    display: block;
    font-family: Microsoft Yahei;
    font-size: 2rem;
    width: 100px;
    text-align: center;
}
.nav-top span {
    float: left;
    display: block;
    font-family: Microsoft Yahei;
    font-size: 2rem;
    width: 80px;
    text-align: center;
}

.nav-top .bi-nav {
    font-size: 1.2rem;
    position: relative;
}

.nav-top .bi-nav .active {
    border-top: solid 4px #fd7855;
    color: #fd7855;
    background: #393b3a;
}

.nav-top .bi-nav .active:after {
    content: '\00a0';
    border-color: #393b3a transparent transparent transparent;
    border-width: 10px;
    border-style: solid;
    width: 0;
    height: 0;
    position: absolute;
    left: 39px;
    bottom: -14px;
}

.nav-top .bi-nav a {
    color: #bdbdbd;
    border-top: solid 4px #343342;
    line-height: 46px;
    display: inline-block;
    font-size: 1.6rem;
    width: 100%;
    border-left: solid 1px #252525;
    border-right: solid 1px #3e4146;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.nav-top span a {
    color: #6a6a6a;
}

.nav-top .nav-topright {
    float: right;
    border-top: solid 4px transparent;
}

.nav-top .nav-topright .triangle_border {
    position: relative;
    display: inline-block;
    text-align: center;
    padding: 0 20px;
    height: 46px;
}

.nav-top .nav-topright .triangle_border:hover {
    background-color: #393b3a;
}

.nav-top .nav-topright .triangle_border i {
    width: 30px;
    height: 30px;
    background: url(/anaweb/images/setting.png);
    display: inline-block;
    margin-top: 7px;
    cursor: pointer;
}

.nav-top .nav-topright .triangle_border a {
    color: #fff;
    text-decoration: none;
}

.nav-top .nav-topright .dropdown-menu-header a {
    line-height: 30px;
    min-width: 80px;
    width: 100%;
    margin-right: 0px;
    text-align: center;
    margin-left: 0px;
    color: #bdbdbd;
    display: block;
}
.nav-top .nav-topright .dropdown-menu-header a:hover {
    background-color: #fd7855;
    color: #fff;
}



.nav-top .nav-topright .triangle_border .popup {
    display: none;
    width: 100px;
    position: absolute;
    top: 46px;
    left: -27px;
    z-index: 1;
}

