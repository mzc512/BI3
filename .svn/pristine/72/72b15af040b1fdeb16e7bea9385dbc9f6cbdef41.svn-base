html, body, div, strong, pre, table, tr, td {
    margin: 0;
    padding: 0;
}
a{
    color: #000;
}
.resume_div_wrap {
    text-align: left;
    font-family: Arial, "\5b8b\4f53";
    font-size: 12px;
    line-height: 22px;
    color: #333;
    margin: 0 auto;
    overflow: hidden;
    word-break: normal;
    word-wrap: break-word;
    padding-bottom: 50px;
}

.resume_div_wrap table, .resume_div_wrap td {
    font-family: Arial, "\5b8b\4f53";
    font-size: 12px;
    line-height: 22px;
    color: #333;
}

.resume_div_wrap td.common_left {
    width: 8%;
    text-align: right;
    color: #666;
    white-space: nowrap;
}
.common_right_r {
    width: 43%;
}

.common_con {
    padding: 15px 25px 0;
}

.common_box {
    border-left: #ddd solid 1px;
    border-right: #ddd solid 1px;
}

.resume_div_wrap td.common_tit ,.common_tit {
    background: #fafafa;
    color: #fd7855;
    padding: 0 25px;
    height: 20px;
    border: solid 1px #e4e4e4;
}

.common_tit {
    cursor: pointer;
    height: 24px;
}

.hyperlinks, .hyperlinks:link, .hyperlinks:visited, .hyperlinks:active {
    color: #333;
    text-decoration: none;
}

.searchbox {
    position: relative;
    height: 46px;
}
.searchbox-warp{
    position: absolute;
    top: 6px;
    right: 14px;
}
.searchbox-warp span{
    font-size: 13px;
}

.searchbox-warp input {
    border: none;
    border-bottom: 1px solid #dddddd;
    vertical-align: top;
    height: 30px;
    font-size: 13px;
    width: 200px;
    background-color: transparent;
}

.searchbox-warp .f-blue-btn {
    display: inline-block;
    background: #7FDBFF;
    height: 30px;
    line-height: 30px;
    text-align: center;
    font-size: 13px;
    border: none;
    color: #fff;
    border-radius: 3px;
    cursor: pointer;
    padding: 0 10px;
    vertical-align: top;
}


.resume_div_wrap td.comlumn8 {
    width: 8%;
    text-align: center;
    color: #666;
    white-space: nowrap;
}
.resume_div_wrap td.comlumn40 {
    width: 40%;
    text-align: center;
    color: #666;
    white-space: nowrap;
}
.resume_div_wrap td.comlumn12 {
    width: 12%;
    text-align: center;
    color: #666;
    white-space: nowrap;
}

.resume_div_wrap td.comlumn12_5 {
    width: 12%;
    text-align: center;
    color: #666;
    white-space: nowrap;
    border: 1px solid;
}
.icon-double-angle-down {
    display: none;
}
.redsinput{
    color: red;
}
.bluesinput{
    color: blue;
}